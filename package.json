  "main": "index.js",
  "scripts": {
    "test": "node ./tests/test-runner.js | tap-spec",
    "build": "babel src --presets babel-preset-es2015 --out-dir es5dist",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/grantcarthew/node-scalable-blob-store.git"
  },
  "bugs": {
