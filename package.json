    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.5.0",
    "crispy-stream": "^0.1.2",
    "mock-fs": "^3.7.0",
    "moment": "^2.11.2",
    "node-dir": "^0.1.11",
    "standard": "^6.0.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "bluebird": "^3.2.2",
    "cuid": "^1.3.8",
    "fs-blob-store": "^5.2.1",
    "mkdirp": "^0.5.1",
    "node-uuid": "^1.4.7"
  }
