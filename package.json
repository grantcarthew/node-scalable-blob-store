{
  "name": "scalable-blob-store",
  "version": "3.0.9",
  "description": "A simple local file system blob store that is designed to prevent conflicts when used with a distributed or replicated file system.",
  "homepage": "https://github.com/grantcarthew/node-scalable-blob-store",
  "author": "Grant Carthew <dev@carthew.net> (http://uglygizmo.blogspot.com.au/)",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/grantcarthew/node-scalable-blob-store.git"
  },
  "bugs": {
    "url": "https://github.com/grantcarthew/node-scalable-blob-store/issues"
  },
  "keywords": [
    "blob-store",
    "storage",
    "blob",
    "store",
    "file",
    "scalable",
    "uuid",
    "guid",
    "cuid",
    "distributed",
    "local"
  ],
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "standard",
    "upgrade": "npm run upgrade:rm && npm run upgrade:ncu && npm run upgrade:install && npm run upgrade:finish",
    "upgrade:rm": "rm -Rf node_modules",
    "upgrade:ncu": "npm-check-updates --upgradeAll",
    "upgrade:install": "npm install",
    "upgrade:finish": "npm run test"
  },
  "jest": {
    "verbose": true,
    "globalSetup": "./tests/jest-setup.js",
    "globalTeardown": ""
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": "dist",
    "globals": [
      "describe",
      "test",
      "expect",
      "beforeAll",
      "jest"
    ]
  },
  "devDependencies": {
    "crispy-stream": "^0.1.2",
    "del": "^3.0.0",
    "jest": "^23.6.0",
    "node-dir": "^0.1.17",
    "npm-check-updates": "^2.14.2",
    "standard": "^12.0.1",
    "ulid": "^2.3.0",
    "cuid": "^2.1.4",
    "uuid": "^3.3.2"
  },
  "dependencies": {}
}
